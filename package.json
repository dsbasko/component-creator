{
  "name": "create-component-helper",
  "displayName": "Component Creator",
  "description": "üß¨ Create components for any language and any framework",
  "version": "2.1.7",
  "engines": {
    "vscode": "^1.70.0"
  },
  "icon": "assets/icon.png",
  "license": "MIT",
  "publisher": "dsbasko",
  "author": {
    "name": "Dmitriy Basenko"
  },
  "homepage": "https://github.com/dsbasko/component-creator",
  "repository": {
    "type": "git",
    "url": "https://github.com/dsbasko/component-creator"
  },
  "bugs": {
    "url": "https://github.com/dsbasko/component-creator/issues"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "File create",
    "File structure",
    "Create structure",
    "Create component",
    "Python component",
    "React component",
    "Vue component",
    "Angular component"
  ],
  "activationEvents": [
    "onCommand:component-creator.AddComponent",
    "onCommand:component-creator.DownloadTemplate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "component-creator.AddComponent",
        "title": "New component üß©"
      },
      {
        "command": "component-creator.DownloadTemplate",
        "title": "Component Creator: Download Template"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "component-creator.AddComponent",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "component-creator.AddComponent",
          "group": "navigation",
          "when": "editorHasSelection"
        }
      ]
    },
    "configuration": {
      "title": "—Åomponent-—Åreator",
      "properties": {
        "componentCreatorHelper.templateDirectory": {
          "type": "string",
          "default": "./.vscode/cch-template",
          "description": "The directory where the templates will be stored."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.185",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vsce": "^2.11.0"
  }
}